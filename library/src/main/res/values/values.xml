<?xml version="1.0" encoding="utf-8"?>

<resources>


    <attr name="title_text" format="string" />
    <attr name="title_start_img" format="reference" />
    <attr name="title_end_img" format="reference" />
    <attr name="title_end_text" format="string" />
    <attr name="title_end_color" format="color" />
    <attr name="title_end_size" format="dimension" />
    <attr name="title_bg_color" format="color" />
    <attr name="title_text_color" format="color" />

    <declare-styleable name="BaseTitle">
        <attr name="title_text" />
        <attr name="title_text_color" />
        <attr name="title_start_img" />
        <attr name="title_end_img" />
        <attr name="title_end_text" />
        <attr name="title_end_color" />
        <attr name="title_end_size" />
        <attr name="title_bg_color" />
    </declare-styleable>


    <attr name="tab_text" format="string" />
    <attr name="tab_resource" format="reference" />

    <declare-styleable name="MainTabView">
        <attr name="tab_text" />
        <attr name="tab_resource" />
    </declare-styleable>


    <declare-styleable name="SwitchButton">
        <attr name="switchOpenImage" format="reference" />
        <attr name="switchCloseImage" format="reference" />
        <attr name="switchStatus">
            <enum name="open" value="0" />
            <enum name="close" value="1" />
        </attr>
    </declare-styleable>

    <attr name="rv_backgroundColor" format="color" />
    <attr name="rv_backgroundPressColor" format="color" />
    <attr name="rv_cornerRadius" format="dimension" />
    <attr name="rv_cornerRadius_BL" format="dimension" />
